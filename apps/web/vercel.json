{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_GOOGLE_CLIENT_ID": "disabled-for-development",
    "VITE_API_BASE_URL": "https://aether-calendar-api.vercel.app/api/v1"
  }
}